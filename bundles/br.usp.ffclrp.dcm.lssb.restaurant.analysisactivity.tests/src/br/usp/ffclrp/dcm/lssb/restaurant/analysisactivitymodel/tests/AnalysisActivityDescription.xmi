<?xml version="1.0" encoding="UTF-8"?>
<aad:AnalysisActivityDescription
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:aad="http://dcm.ffclrp.usp.br/lssb/metamodels/analysis-activity-description.ecore"
    xmlns:slm="http://dcm.ffclrp.usp.br/lssb/metamodels/string-list-manipulator.ecore">
  <parameters
      name="email"/>
  <parameters
      name="count"
      parameterType="INTEGER">
    <defaultValue>5</defaultValue>
  </parameters>
  <parameters
      name="threshold"
      parameterType="REAL">
    <defaultValue>2.0</defaultValue>
  </parameters>
  <parameters
      name="gene-ids"
      parameterKind="LIST"/>
  <parameters
      name="categories-in-result"
      parameterKind="LIST"/>
  <parameters
      name="gene-identifier-type"/>
  <outputDatasets
      name="output.tsv"
      mimetype="text/tsv"
      datasetKind="SINGLE_FILE"/>
  <commandLineTemplate
      xsi:type="aad:LiteralCommandLineEntryList">
    <literals>geas-david-chart-report.py</literals>
  </commandLineTemplate>
  <commandLineTemplate
      xsi:type="aad:ParameterCommandLineEntryList"
      parameter="//@parameters.0"/>
  <commandLineTemplate
      xsi:type="aad:ParameterCommandLineEntryList"
      parameter="//@parameters.3">
    <manipulators
        xsi:type="slm:Join"
        delimiter=","/>
  </commandLineTemplate>
  <commandLineTemplate
      xsi:type="aad:ParameterCommandLineEntryList"
      parameter="//@parameters.5"/>
  <commandLineTemplate
      xsi:type="aad:ParameterCommandLineEntryList"
      parameter="//@parameters.4">
    <manipulators
        xsi:type="slm:Join"
        delimiter=","/>
  </commandLineTemplate>
  <commandLineTemplate
      xsi:type="aad:ParameterCommandLineEntryList"
      parameter="//@parameters.2"/>
  <commandLineTemplate
      xsi:type="aad:ParameterCommandLineEntryList"
      parameter="//@parameters.1"/>
  <commandLineTemplate
      xsi:type="aad:DatasetCommandLineEntryList"
      dataset="//@outputDatasets.0"/>
  <readinessContraints
      xsi:type="aad:MinimunParameterCardinalityConstraint"
      parameter="//@parameters.0"
      value="1"/>
  <readinessContraints
      xsi:type="aad:MinimunParameterCardinalityConstraint"
      parameter="//@parameters.1"
      value="1"/>
  <readinessContraints
      xsi:type="aad:MinimunParameterCardinalityConstraint"
      parameter="//@parameters.2"
      value="1"/>
  <readinessContraints
      xsi:type="aad:MinimunParameterCardinalityConstraint"
      parameter="//@parameters.3"
      value="1"/>
  <readinessContraints
      xsi:type="aad:MinimunParameterCardinalityConstraint"
      parameter="//@parameters.4"
      value="1"/>
  <readinessContraints
      xsi:type="aad:MinimunParameterCardinalityConstraint"
      parameter="//@parameters.5"
      value="1"/>
</aad:AnalysisActivityDescription>
