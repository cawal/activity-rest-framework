activity my-activity {
	on {
		dataset 'dummy-dataset' : 'text/plain' [1,1];
	}
	with {
		parameter 'dummy-parameter' : STRING [1,1];
	}
	produces {
		dataset 'output' : 'text/plain' [1,1];
	}
	using executable 'my-tool' {
		commandLineTemplate [ 
			parameter 'dummy-parameter',
			dataset 'dummy-dataset',
			dataset 'output'
		]
	}
}
